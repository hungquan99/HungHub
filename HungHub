local OtherSection = OtherTab:CreateSection("Misc")

local Slider = OtherTab:CreateSlider({
    Name = "Walk Speed",
    Range = {1, 500},
    Increment = 1,
    Suffix = "Speed",
    CurrentValue = 1,
    Flag = "Slider1", 
    Callback = function(Value)
        game.Players.LocalPlayer.Character:SetAttribute("SpeedMultiplier", Value)
    end,
 })

 local Slider = OtherTab:CreateSlider({
    Name = "Dash length",
    Range = {1, 500},
    Increment = 1,
    Suffix = "Length",
    CurrentValue = 1,
    Flag = "Slider2", 
    Callback = function(Value)
        game.Players.LocalPlayer.Character:SetAttribute("DashLength", Value)
    end,
 })

 local Slider = OtherTab:CreateSlider({
    Name = "Height",
    Range = {1, 500},
    Increment = 1,
    Suffix = "Height",
    CurrentValue = 1,
    Flag = "Slider3", 
    Callback = function(Value)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = Value
    end,
 })

 local RaidTab = Window:CreateTab("Raid", 4483362458)
 local RaidSection = RaidTab:CreateSection("Raid")

 local RaidToggle = RaidTab:CreateToggle({
    Name = "Kill Aura",
    CurrentValue = false,
    Flag = "Kill Aura", 
    Callback = function(Value)
        for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
            if v.Name == "HumanoidRootPart" then
            v.Parent.Humanoid.Health = 0
            v.Parent.HumanoidRootPart.Size = Vector3.new(20,20,20)
            v.Parent.HumanoidRootPart.CanCollide = false
            v.Parent.HumanoidRootPart.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame * CFrame.new(0,20,0)
            v.Parent.HumanoidRootPart.Size = Vector3.new(2,2,1)
            sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
            end
            end
    end,
 })
